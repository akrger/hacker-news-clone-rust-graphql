{
  "name": "frontend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "postinstall": "cd .. && husky install frontend/.husky",
    "start": "yarn relay && react-native start",
    "start:storybook": "START_WITH_STORYBOOK=true yarn start",
    "download-schema": "npx graphql-codegen --require dotenv/config",
    "relay": "yarn download-schema && relay-compiler --language typescript --src ./src --schema ./schema.graphql --extensions ts tsx",
    "relay:watch": "yarn relay --watch",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "storybook": "start-storybook -p 7007",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@apollo/client": "3.3.15",
    "@react-native-async-storage/async-storage": "1.15.3",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/drawer": "5.12.5",
    "@react-navigation/native": "5.9.4",
    "@react-navigation/stack": "5.14.4",
    "@shopify/restyle": "1.4.0",
    "@xstate/react": "1.3.2",
    "date-fns": "2.21.1",
    "i18next": "20.2.1",
    "react": "17.0.1",
    "react-i18next": "11.8.13",
    "react-native": "0.64.0",
    "react-native-config": "1.4.2",
    "react-native-gesture-handler": "1.10.3",
    "react-native-reanimated": "2.1.0",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "3.1.1",
    "react-native-vector-icons": "8.1.0",
    "react-relay": "11.0.2",
    "relay-runtime": "11.0.2",
    "xstate": "4.18.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.9",
    "@babel/runtime": "7.12.5",
    "@graphql-cli/codegen": "1.17.23",
    "@graphql-codegen/schema-ast": "1.18.2",
    "@storybook/addon-actions": "^5.3",
    "@storybook/addon-knobs": "^5.3",
    "@storybook/addon-links": "^5.3",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.25",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "@types/jest": "26.0.20",
    "@types/react": "17.0.3",
    "@types/react-native": "0.64.0",
    "@types/react-native-vector-icons": "6.4.6",
    "@types/react-relay": "11.0.1",
    "@types/react-test-renderer": "16.9.2",
    "@types/relay-runtime": "11.0.0",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "babel-jest": "26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-relay": "11.0.2",
    "babel-plugin-transform-inline-environment-variables": "0.4.3",
    "eslint": "7.24.0",
    "eslint-config-prettier": "8.2.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "graphql": "15.5.0",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "metro-react-native-babel-preset": "0.64.0",
    "prettier": "2.2.1",
    "react-dom": "17.0.1",
    "react-test-renderer": "17.0.1",
    "relay-compiler": "11.0.2",
    "relay-compiler-language-typescript": "14.0.0",
    "typescript": "4.2.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --cache --fix",
      "node scripts/createTempTSConfig.js"
    ],
    "*.js": "eslint --cache --fix",
    "*.json": "prettier --write"
  }
}
