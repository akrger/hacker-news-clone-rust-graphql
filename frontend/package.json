{
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "prestart": "npm run download-schema && npm run relay",
    "start": "npm run relay -- --watch & react-native start",
    "test": "jest",
    "lint": "tsc && eslint .",
    "update-all": "ncu -u && npm update",
    "download-schema": "graphql-codegen --require dotenv/config",
    "prepare": "cd .. && husky install frontend/.husky",
    "relay": "relay-compiler --language typescript --src ./src --schema ./schema.graphql --extensions ts tsx"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.15.2",
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-cookies/cookies": "6.0.7",
    "@react-navigation/drawer": "5.12.5",
    "@react-navigation/native": "5.9.4",
    "@react-navigation/stack": "5.14.4",
    "@shopify/restyle": "1.4.0",
    "react": "17.0.1",
    "react-native": "0.64.0",
    "react-native-config": "1.4.2",
    "react-native-gesture-handler": "1.10.3",
    "react-native-logs": "3.0.3",
    "react-native-reanimated": "2.1.0",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "3.1.1",
    "react-native-vector-icons": "8.1.0",
    "react-relay": "11.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.13.15",
    "@babel/runtime": "7.13.10",
    "@graphql-cli/codegen": "1.17.23",
    "@graphql-codegen/schema-ast": "1.18.2",
    "@react-native-community/eslint-config": "2.0.0",
    "@types/jest": "26.0.22",
    "@types/react": "17.0.3",
    "@types/react-native": "0.64.4",
    "@types/react-native-vector-icons": "6.4.6",
    "@types/react-relay": "11.0.1",
    "@types/react-test-renderer": "17.0.1",
    "@types/relay-runtime": "11.0.0",
    "babel-jest": "26.6.3",
    "babel-plugin-relay": "11.0.2",
    "eslint": "7.24.0",
    "eslint-config-prettier": "8.2.0",
    "graphql": "15.5.0",
    "graphql-cli": "4.1.0",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "metro-react-native-babel-preset": "0.65.2",
    "npm-check-updates": "11.4.1",
    "react-test-renderer": "17.0.1",
    "relay-compiler": "11.0.2",
    "relay-compiler-language-typescript": "14.0.0",
    "relay-config": "11.0.2",
    "typescript": "4.2.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --cache --fix",
      "node scripts/createTempTSConfig.js"
    ],
    "*.js": "eslint --cache --fix",
    "*.json": "prettier --write"
  }
}
