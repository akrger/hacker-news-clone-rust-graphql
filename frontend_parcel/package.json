  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "10.4.1",
    "parcel": "2.1.1",
    "parcel-namer-rewrite": "2.0.0-rc.1",
    "parcel-reporter-bundle-manifest": "1.0.0",
    "postcss": "8.4.5",
    "tailwindcss": "3.0.11"
  },
  "dependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "parcel-namer-rewrite": {
    "chain": "@parcel/namer-default",
    "rules": {
      "(.*)\\.(js)": "js/$1.{hash}.$2"
    }
  }
