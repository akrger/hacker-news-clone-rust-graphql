    "parcel:dev": "parcel src/templates/admin.html src/templates/guestbook.html",
    "parcel:build": "parcel build src/templates/admin.html src/templates/guestbook.html --dist-dir ../backend/static --public-url static",
    "parcel:build-unoptimized": "parcel build src/templates/admin.html src/templates/guestbook.html --dist-dir ../backend/static --public-url static --no-content-hash --no-optimize",
